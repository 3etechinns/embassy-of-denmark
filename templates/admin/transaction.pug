doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no")
    meta(http-equiv="x-ua-compatible" content="ie=edge")
    title Embassy Of Denmark
    // Font Awesome
    link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css")
    // Bootstrap core CSS
    link(href="css/bootstrap.min.css" rel="stylesheet")
    // Material Design Bootstrap
    link(href="css/mdb.min.css" rel="stylesheet")
    // Your custom styles (optional)
    link(href="css/style.css" rel="stylesheet")
    link(href="css/compiled.min.css" rel="stylesheet")
  body.fixed-sn.mdb-skin
    // Main Navigation
    header
      // Navbar
      nav.navbar.navbar-expand-lg.navbar-dark.double-nav.fixed-top.scrolling-navbar
        // SideNav slide-out button
        .float-left
          a.button-collapse(href="#" data-activates="slide-out")
            i.fa.fa-bars
        // Breadcrumb
        .breadcrumb-dn.mr-auto
          p(style="font-weight: 600; font-size: 13px;") ADMIN DASHBOARD
        // Links
        ul.nav.navbar-nav.nav-flex-icons.ml-auto
          li.nav-item
            a.nav-link(href="support.html")
              i.fa.fa-comments-o
              span.clearfix.d-none.d-sm-inline-block support
          li.nav-item
            a.nav-link
              i.fa.fa-gear
              span.clearfix.d-none.d-sm-inline-block Settings
          li.nav-item.dropdown
            a.nav-link
              i.fa.fa-power-off
      // /.Navbar
      // Sidebar navigation
      #slide-out.side-nav.fixed(style="background: url(img/logo.png) no-repeat;\
      background-position: center center;\
      background-size: cover;\
      background-attachment: fixed;\
      background-color: black;")
        ul.custom-scrollbar.list-unstyled
          // Logo
          li.logo-sn.waves-effect
            .text-center
              a.pl-0(href="#")
                img(src="img/logo.png")
          // /. Logo
          // Search Form
          li
            form.search-form(role="search")
              .form-group.waves-light
                input.form-control(type="text" placeholder="Search")
          // /.Search Form
          // Side navigation links
          li
            ul.collapsible.collapsible-accordion
              li
                a.waves-effect.arrow-r(href="index.html")
                  | Dashboard
              hr(style="margin: 10px 0px;")
              li
                a.collapsible-header.waves-effect.arrow-r
                  | Transaction Pipeline
                  i.fa.fa-angle-down.rotate-icon
                .collapsible-body
                  ul
                    li
                      a.waves-effect(href="transaction.html") All
                    li
                      a.waves-effect(href="transaction.html") New Requests
                    li
                      a.waves-effect(href="transaction.html") Under Processing
                    li
                      a.waves-effect(href="transaction.html") Completed Requests
                    li
                      a.waves-effect(href="transaction.html") Dispatched Requests
              hr(style="margin: 10px 0px;")
              li
                a.waves-effect.arrow-r(href="user_search.html")
                  | User Search & Reporting
              hr(style="margin: 10px 0px;")
              li
                a.waves-effect.arrow-r(href="embassy_requests.html")
                  | Inter-Embassy Requests
              hr(style="margin: 10px 0px;")
              li
                a.collapsible-header.waves-effect.arrow-r
                  | Live Chat
                  i.fa.fa-angle-down.rotate-icon
                .collapsible-body
                  ul
                    li
                      a.waves-effect(href="#") FAQ
                    li
                      a.waves-effect(href="support.html") Write to support
                    li
                      a.waves-effect(href="#") Chat with a collegue
                    li
                      a.waves-effect(href="#") Chat with other Embassies
              hr(style="margin: 10px 0px;")
              li
                a.collapsible-header.waves-effect.arrow-r
                  | Settings
                  i.fa.fa-angle-down.rotate-icon
                .collapsible-body
                  ul
                    li
                      a.waves-effect(href="#") Access Rights
                    li
                      a.waves-effect(href="#") User log
                    li
                      a.waves-effect(href="#") Currency
                    li
                      a.waves-effect(href="#") Bank/Card details
                    li
                      a.waves-effect(href="#") Embassy Profile
                    li
                      a.waves-effect(href="#") Staff
          // /. Side navigation links
        // Mask
        .sidenav-bg.mask-strong
      // /. Sidebar navigation
    // Main Navigation
    // Main layout
    main(style="margin: 0%;")
      .container-fluid.row(style="margin: 0px;")
        .col-md-12
          section
            h6(style="font-weight:600;") TRANSACTION SUMMARY:
            .row.mt-2.mb-2
              .col-md-3
                h6 New Requests
                h1 0
              .col-md-3
                h6 Under Processing
                h1 0
              .col-md-3
                h6 Completed Requests
                h1 0
              .col-md-3
                h6 Dispatched Requests
                h1 0
          hr
          // Section: Modals
          section
            // Modal: modalConfirmDelete
            #modalConfirmDelete.modal.fade(tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true")
              .modal-dialog.modal-sm.modal-notify.modal-danger(role="document")
                // Content
                .modal-content.text-center
                  // Header
                  .modal-header.d-flex.justify-content-center
                    p.heading Are you sure?
                  // Body
                  .modal-body
                    p You can't Undo once this item is deleted
                  // Footer
                  .modal-footer.flex-center
                    a.btn.btn-outline-secondary-modal(style="color: black !important;") Yes
                    a.btn.btn-primary-modal.waves-effect(type="button" data-dismiss="modal" style="color: black !important;") No
                // /.Content
            // Modal: modalConfirmDelete
          // Section: Modals
          // Section: Table
          section
            // Card
            .card.card-cascade.narrower(style="width: 100%;")
              // Card header
              .view.py-3.gradient-card-header.blue.mx-4.d-flex.justify-content-between.align-items-center
                a.white-text.mx-3(href="") Transaction Pipeline
                div
                  button.btn.btn-outline-white.btn-rounded.btn-sm.px-2(type="button" data-toggle="modal" data-target="#modalConfirmDelete")
                    i.fa.fa-remove.mt-0
                  button.btn.btn-outline-white.btn-rounded.btn-sm.px-2(type="button" data-toggle="tooltip" data-placement="bottom" title="info")
                    i.fa.fa-info-circle.mt-0
              // /Card header
              // Card content
              .card-body
                .row
                  .col-md-6.my-2
                    .custom-select(style="width: 50%;")
                      select
                        option(value="" disabled="" selected="") show all
                        option(value="0") show all
                        option(value="1") New Requests
                        option(value="2") Under Processing
                        option(value="3") Completed Requests
                        option(value="3") Dispatched Requests
                  .col-md-6.my-2
                    form.form-inline
                      input.form-control.my-0.py-0(type="text" placeholder="Search" style="max-width: 150px;")
                      button.btn.btn-sm.btn-info.ml-2.px-2
                        i.fa.fa-search
                .table-responsive
                  table.table
                    thead
                      tr
                        th
                          input#checkbox(type="checkbox")
                          label.label-table(for="checkbox")
                        th.th-lg Transaction ID
                        th.th-lg Nature
                        th.th-lg Date Initiated
                        th.th-lg Status
                    tbody
                      tr
                        th(scope="row")
                          input#checkbox1(type="checkbox")
                          label.label-table(for="checkbox1")
                        td Lorem Ipsum
                        td Lorem Ipsum
                        td Lorem Ipsum
                        td Lorem Ipsum
                      tr
                        th(scope="row")
                          input#checkbox2(type="checkbox")
                          label.label-table(for="checkbox2")
                        td Lorem Ipsum
                        td Lorem Ipsum
                        td Lorem Ipsum
                        td Lorem Ipsum
                      tr
                        th(scope="row")
                          input#checkbox3(type="checkbox")
                          label.label-table(for="checkbox3")
                        td Lorem Ipsum
                        td Lorem Ipsum
                        td Lorem Ipsum
                        td Lorem Ipsum
                hr.my-0
                // Bottom Table UI
                div
                  // Name
                  .row
                    .col-md-4.custom-select.my-4.mx-3(style="width: 100%;")
                      select.my-4
                        option(value="0" disabled="") Rows number
                        option(value="1" selected="") 10 rows
                        option(value="2") 25 rows
                        option(value="3") 50 rows
                        option(value="4") 100 rows
                        option(value="4") all
                    .col-md-5
                      // Pagination
                      nav.my-4
                        ul.pagination.pagination-circle.pg-blue.mb-0
                          // First
                          li.page-item.disabled
                            a.page-link First
                          // Arrow left
                          li.page-item.disabled
                            a.page-link(aria-label="Previous")
                              span(aria-hidden="true") «
                              span.sr-only Previous
                          // Numbers
                          li.page-item.active
                            a.page-link 1
                          li.page-item
                            a.page-link 2
                          li.page-item
                            a.page-link 3
                          // Arrow right
                          li.page-item
                            a.page-link(aria-label="Next")
                              span(aria-hidden="true") »
                              span.sr-only Next
                          // First
                          li.page-item
                            a.page-link Last
                      // /Pagination
                // Bottom Table UI
              // /.Card content
            // /.Card
          // Section: Table
    // Main layout
    // Footer
    footer.page-footer.center-on-small-only
      .flex-center.mb-3.mt-2
        a.px-3(href="")
          i.fa.fa-facebook
        a.px-3(href="")
          i.fa.fa-instagram
        a.px-3(href="")
          i.fa.fa-google
        a.px-3(href="")
          i.fa.fa-twitter
        a.px-3(href="")
          i.fa.fa-linkedin
      // Copyright
      .footer-copyright
        .container-fluid
          | © 2018 Copyright:
          a(href="https://www.MDBootstrap.com")  Embassy Of Denmark 
      // /.Copyright
    // /.Footer
    // SCRIPTS
    // JQuery
    script(type="text/javascript" src="js/jquery-3.3.1.min.js")
    // Bootstrap tooltips
    script(type="text/javascript" src="js/popper.min.js")
    // Bootstrap core JavaScript
    script(type="text/javascript" src="js/bootstrap.min.js")
    // MDB core JavaScript
    script(type="text/javascript" src="js/mdb.min.js")
    script(type="text/javascript" src="js/compiled.min.js")
    script(type="text/javascript" src="js/custom.js")
    // Initializations
    script.
      // SideNav Initialization
      $(".button-collapse").sideNav();
      // Material Select Initialization
      $(document).ready(function () {
      $('.mdb-select').material_select();
      });
      // Data Picker Initialization
      $('.datepicker').pickadate();
      // Tooltip Initialization
      $(function () {
      $('[data-toggle="tooltip"]').tooltip()
      })
    // Charts
    script.
      // Minimalist chart
      $(function () {
      $('.min-chart#chart-revenue').easyPieChart({
      barColor: "#4caf50",
      onStep: function (from, to, percent) {
      $(this.el).find('.percent').text(Math.round(percent));
      }
      });
      });
      // Main chart
      var ctxL = document.getElementById("lineChart").getContext('2d');
      var myLineChart = new Chart(ctxL, {
      type: 'line',
      data: {
      labels: ["January", "February", "March", "April", "May", "June", "July"],
      datasets: [{
      label: "Revenue Graph",
      fillColor: "#fff",
      backgroundColor: 'rgba(255, 255, 255, .3)',
      borderColor: 'rgba(255, 99, 132)',
      data: [0, 10, 5, 2, 20, 30, 45],
      }]
      },
      options: {
      legend: {
      labels: {
      fontColor: "#fff",
      }
      },
      scales: {
      xAxes: [{
      gridLines: {
      display: true,
      color: "rgba(255,255,255,.25)"
      },
      ticks: {
      fontColor: "#fff",
      },
      }],
      yAxes: [{
      display: true,
      gridLines: {
      display: true,
      color: "rgba(255,255,255,.25)"
      },
      ticks: {
      fontColor: "#fff",
      },
      }],
      }
      }
      });
    // Alerts
    script.
      $(function () {
      $('#folder-1').click(function () {
      toastr.error("Folder 1 has been clicked!", "Folder 1", {
      "positionClass": "toast-top-right",
      });
      });
      $('#folder-2').click(function () {
      // make it not dissappear
      toastr.info("Folder 2 has been clicked!", "Folder 2", );
      });
      $('#folder-3').click(function () {
      // make it not dissappear
      toastr.info("Folder 3 has been clicked!", "Folder 3", );
      });
      });
    // sticky
    script(type="text/javascript").
      $(function () {
      $(".sticky").sticky({
      topSpacing: 90
      , zIndex: 2
      , stopper: "#stopper"
      });
      });
